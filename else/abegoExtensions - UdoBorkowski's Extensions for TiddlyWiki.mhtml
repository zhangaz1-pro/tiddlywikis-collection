// (see IncludePlugin (http://tiddlywiki.abego-software.de/#IncludePlugin))

var fixUpURL =3D function(url) {
	// Make sure the URL is a real URL, with protocol prefix etc.
	if (url.search(/^((http(s)?)|(file)):/) !=3D 0) {
	=09
		// no protocol specified.=20
		if (url.search(/^((.\:\\)|(\\\\)|(\/))/) =3D=3D 0) {
			// "url" is an "absolute" path to a local file. Prefix it with file://
			url =3D "file://"+url;
		=09
		} else {
			// "url" is a "relative" URL. Make it absolute
		=09
			// prefix the url with the directory containing the current document
			// (This also includes the protocol prefix)
			var documentPath =3D document.location.toString();
			var i =3D documentPath.lastIndexOf("/");
			url =3D documentPath.substr(0,i+1)+url;
		}
		// replace every \ by a /, to cover Windows style pathes
		url =3D url.replace(/\\/mg,"/");
	}
	return url;
};

// @return [may be null/undefined]
var getIncludedTiddlerURL =3D function(title) {
	var t =3D store.fetchTiddler(title);
	if (!t) return;
	var url =3D t["includeURL"];
	if (!url) return;
	return fixUpURL(url)+"#"+encodeURIComponent(String.encodeTiddlyLink(title)=
);
};

config.commands.editIncludedTiddler =3D {
	text: "goto",
	tooltip: "Open tiddler from an included TiddlyWiki in a separate window. e=
.g. to edit it"
};

var editIncludedTiddlerWindow;=20

config.commands.editIncludedTiddler.handler =3D function(event,src,title) {
	var url =3D getIncludedTiddlerURL(title);
	if (url) {
		if (editIncludedTiddlerWindow) editIncludedTiddlerWindow.close();
		editIncludedTiddlerWindow =3D window.open(url,"editIncludedTiddlerWindow"=
);
	} else {
		displayMessage("Tiddler is not from an included TiddlyWiki");
	}
};

var orig_getValue =3D TiddlyWiki.prototype.getValue;
// see 'Applications of "getValue"' below
TiddlyWiki.prototype.getValue =3D function(tiddler,fieldName) {
	var result =3D orig_getValue.apply(this,arguments);
	if (result =3D=3D=3D undefined) {
		var t =3D this.resolveTiddler(tiddler);

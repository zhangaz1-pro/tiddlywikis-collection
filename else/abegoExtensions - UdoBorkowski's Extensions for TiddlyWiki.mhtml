var parseStartTagParams =3D function(paramText) {
	var params =3D paramText.readMacroParams();
	if (params.length =3D=3D 0 || params[0].length =3D=3D 0) return null;
=09
	var name =3D params[0];
	var paramsIndex =3D 1;
	var hidden =3D false;
	if (paramsIndex &lt; params.length) {
		hidden =3D params[paramsIndex] =3D=3D "hidden";
		paramsIndex++;
	}
=09
	return {
		partName: name,=20
		isHidden: hidden
	};
}

// Returns the match to the next (end or start) part tag in the text,=20
// starting the search at startIndex.
//=20
// When no such tag is found null is returned, otherwise a "Match" is retur=
ned:
// [0]: full match
// [1]: matched "end" tag (or null when no end tag match)
// [2]: matched "start" tag (or null when no start tag match)
// [3]: content of start tag (or null if no start tag match)
//
var findNextPartEndOrStartTagMatch =3D function(text, startIndex) {
	var re =3D new RegExp(partEndOrStartTagRE);
	re.lastIndex =3D startIndex;
	var match =3D re.exec(text);
	return match;
}


// ------------------------------------------------------------------------=
---
// Helpers
// ------------------------------------------------------------------------=
---

var indexInArray =3D function(array, item) {
	for (var i =3D 0; i &lt; array.length; i++) {
		if (array[i] =3D=3D item) {
			return i;
		}
	}
	return -1;
}

var myEscapeRegExp =3D function(s)
{
	// The original escapeRegExp function does not work with Safari (2.0.3)=20
	// since the $&amp; is not implemented.
var t =3D s.replace(/\\/g, "\\\\");
	t =3D t.replace(/\^/g, "\\^");
	t =3D t.replace(/\$/g, "\\$");
	t =3D t.replace(/\*/g, "\\*");
	t =3D t.replace(/\+/g, "\\+");
	t =3D t.replace(/\?/g, "\\?");
	t =3D t.replace(/\(/g, "\\(");
	t =3D t.replace(/\)/g, "\\)");
	t =3D t.replace(/\=3D/g, "\\=3D");
	t =3D t.replace(/\!/g, "\\!");
	t =3D t.replace(/\|/g, "\\|");
	t =3D t.replace(/\,/g, "\\,");
	t =3D t.replace(/\{/g, "\\{");
	t =3D t.replace(/\}/g, "\\}");
	t =3D t.replace(/\[/g, "\\[");
	t =3D t.replace(/\]/g, "\\]");
	t =3D t.replace(/\./g, "\\.");

    return t;
}

// ------------------------------------------------------------------------=

var findPartStartTagByName =3D function(text, partName) {
	var i =3D 0;
=09
	while (true) {
		var tagMatch =3D findNextPartEndOrStartTagMatch(text, i);
		if (!tagMatch) return null;

		if (tagMatch[2]) {
			// Is start tag
=09
			// Check the name
			var arguments =3D parseStartTagParams(tagMatch[3]);
			if (arguments &amp;&amp; arguments.partName =3D=3D partName) {
				return tagMatch;
			}
		}
		i =3D tagMatch.index+tagMatch[0].length;
	}
}

// Return the part "partName" of the given parentTiddler as a "readOnly" Ti=
ddler=20
// object, using fullName as the Tiddler's title.=20
//
// All remaining properties of the new Tiddler (tags etc.) are inherited fr=
om=20
// the parentTiddler.
//=20
// @return [may be null]
//
var getPart =3D function(parentTiddler, partName, fullName) {
	var text =3D parentTiddler.text;
	var startTag =3D findPartStartTagByName(text, partName);
	if (!startTag) return null;
=09
	var endIndexOfStartTag =3D skipEmptyEndOfLine(text, startTag.index+startTa=
g[0].length);
	var indexOfEndTag =3D text.indexOf(partEndTagString, endIndexOfStartTag);

	if (indexOfEndTag &gt;=3D 0) {
		var partTiddlerText =3D text.substring(endIndexOfStartTag,indexOfEndTag);
		var partTiddler =3D new Tiddler();
		partTiddler.set(
						fullName,
						partTiddlerText,
						parentTiddler.modifier,
						parentTiddler.modified,
						parentTiddler.tags,
						parentTiddler.created);
		partTiddler.abegoIsPartTiddler =3D true;
		return partTiddler;
	}
=09
	return null;
}

// Hijack the store.fetchTiddler to recognize the "part" addresses.
//
var hijackFetchTiddler =3D function() {
	var oldFetchTiddler =3D store.fetchTiddler ;
	store.fetchTiddler =3D function(title) {
		var result =3D oldFetchTiddler.apply(this, arguments);
		if (!result &amp;&amp; title) {
			var i =3D title.lastIndexOf('/');
			if (i &gt; 0) {
				var parentName =3D title.substring(0, i);
				var partName =3D title.substring(i+1);
				var parent =3D (parentName =3D=3D ".")=20
						? store.resolveTiddler(currentParent)
						: oldFetchTiddler.apply(this, [parentName]);

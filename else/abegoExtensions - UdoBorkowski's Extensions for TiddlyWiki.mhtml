		setPauseBetweenFilePolls : function(millis) {
			pauseBetweenFilePolls =3D millis;
		},
	=09
		getPauseBetweenFilePolls : function() {
			return pauseBetweenFilePolls;
		},
	=09
		addSyncFileTiddlers : function() {
			var s =3D store.getTiddlerText("SyncFileTiddlers");
			if (!s)
				return;

			var lines =3D s.split("\n");
			for ( var i =3D 0; i &lt; lines.length; i++) {
				var line =3D lines[i];
				var a =3D line.split('\|');
				if (a.length &gt;=3D 3) {
					var tiddler =3D a[1];
					var filepath =3D a[2];
					var readonly =3D a.length &gt;=3D 4 &amp;&amp; a[3]=3D=3D"readonly";
					SyncFileTiddler.add(tiddler, filepath,readonly);
				}

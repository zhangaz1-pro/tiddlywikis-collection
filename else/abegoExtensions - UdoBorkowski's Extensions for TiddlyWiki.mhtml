	var className =3D null;
	var argsStart =3D -1;
	var doWikify =3D true;

	var iParams =3D 1;
	if (params[iParams] !=3D "asText" &amp;&amp; params[iParams] !=3D "with:")=
 {
		className =3D params[iParams++];
	}
	if (params[iParams] =3D=3D "asText") {
		iParams++;
		doWikify =3D false;
	}
	if (params[iParams] =3D=3D "with:") {
		iParams++;
		argsStart =3D iParams;
	}
=09
	var wrapper =3D createTiddlyElement(place,"span",null,className ? classNam=
e : null,null);
	var text =3D store.getTiddlerText(params[0]);
	if(text) {
		// Check for recursion
		var tiddlerName =3D params[0];
		var stack =3D config.macros.tiddler.tiddlerStack;
		if (stack.find(tiddlerName) !=3D=3D null) return;

		if (argsStart &gt;=3D 0) {
			// The params between the "with:" and the "prefix:" (or the end) are the=
 arguments,
			// The param behind the "prefix:" is the prefix before the placeholder n=
umbers.
			var argsEnd =3D params.length;
			var prefix =3D "$";
			var prefixIndex =3D indexInArray(params, "prefix:");
			if (prefixIndex &gt;=3D argsStart) {
				argsEnd =3D prefixIndex;
				if (prefixIndex &lt; (params.length-1)) {
					prefix =3D params[prefixIndex+1];
				}
			}
			// to avoid any "special RE chars" problems with the prefix string escap=
e all chars.
			prefix =3D myEscapeRegExp(prefix);
		=09
			var args =3D params.slice(argsStart, argsEnd);
			var n =3D Math.min(args.length, 9);
			for (var i =3D 0; i &lt; n; i++) {
				var value =3D args[i];
			=09
				var placeholderRE =3D new RegExp(prefix+(i+1),"mg");
				text =3D text.replace(placeholderRE, value);
			}
		}
		stack.push(tiddlerName);
		try {
			if (doWikify) {
				wikify(text,wrapper,null,store.getTiddler(params[0]));
			} else {
				wrapper.appendChild(document.createTextNode(text));
			}
		} finally {		=09
			stack.pop();
		}
	}
}
config.macros.tiddler.tiddlerStack =3D [];

// End of "install only once"
}


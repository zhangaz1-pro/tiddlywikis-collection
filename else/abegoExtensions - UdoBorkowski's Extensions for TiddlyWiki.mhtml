	function getDaysBetween(date1, date2) {
		date1 =3D new Date(date1.getFullYear(),date1.getMonth(),date1.getDate());
		date2 =3D new Date(date2.getFullYear(),date2.getMonth(),date2.getDate());
		return ((date2.getTime()-date1)/86400000).toFixed();
	}

	// Returns the date given by the string s

!Description
It's minesweeper!

!Syntax/Example usage
{{{&lt;&lt;minesweeper&gt;&gt;}}} or {{{&lt;&lt;minesweeper [width] [height=
] [bombs]&gt;&gt;}}}
&lt;&lt;minesweeper&gt;&gt;

!Notes
* Let me know if you want graphics, or that MSWindows Minesweeper thing whe=
re you can click with both buttons on a numbered square, and it does the th=
inking for you..

!Revision History
* 0.5.0 (2-Oct-05)
** original version (Daniel)
* 0.5.1 (3-Oct-05)
** added the css styles via set~StyleSheet (Simon)
* 0.5.2 (3-Oct-05)
** added parameters to macro and fixed bug preventing detection of win (Sim=
on)
* 0.6 (5-Oct-05)
** Fixed the problem with multiple games on screen at once.
** Cleaned up the JavaScript warning generated when you clicked anywhere bu=
t on a square.
* 0.7 (6-Oct-05)
** Integrated sweet additions from Genesis_mage (genisis329 at gmail dot co=
m) that:
*** allows winning by having all non-mines clicked (without having to mark =
every mine)
*** added a mark button to mark and unmark mines without the keyboard
** Win time now shows tenths of a second.
* 0.71 (10-Oct-05)
** tweaked a style to make the mark button work better in IE
* 0.72 (12-Oct-05)
** worked out how to use a closure as a event handler, which means that the=
 code added in 0.6 could be made a lot simpler.

***/
/*{{{*/

version.extensions.minesweeper =3D {major: 0, minor: 7, revision: 2};

config.macros.minesweeper =3D {};

config.macros.minesweeper.handler =3D function(place,macroName,params) {
    var width =3D params[0];
    var height =3D params[1];
    var bombs =3D params[2];

    if (width =3D=3D undefined) width =3D 9;
    if (height =3D=3D undefined) height =3D width;
    if (bombs =3D=3D undefined) bombs =3D Math.round(width * height /  8)
    if (bombs &gt; width * height) bombs =3D width * height;

    var aGame =3D new MinesweeperGame();

    createTiddlyElement(place,'div',aGame.id,null,'If you see this, Mineswe=
eper is broken.  Let Daniel know (DanielBaird at gmail dot com).');
    aGame.newGame(width, height, bombs);
}
// =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=
=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D=3D
function MinesweeperGame() {
    this.idprefix =3D 'mines';
    this.version =3D '0.72 beta';
    this.id =3D this.idprefix + MinesweeperGame.prototype.nextid;
    MinesweeperGame.prototype.nextid++;
    return this;
}
// -----------------------------------------------------------------------
MinesweeperGame.prototype.nextid =3D 0;
// -----------------------------------------------------------------------
MinesweeperGame.prototype.newGame =3D function(height, width, mines) {
    this.height =3D height;
    this.width =3D width;
    this.mines =3D mines;
    this.total =3D height * width;
    this.markMode =3D false;

    this.startGame();
}
// -----------------------------------------------------------------------
MinesweeperGame.prototype.startGame =3D function() {

    this.gamestate =3D 'ready';
    this.clicks =3D 0;
    this.marks =3D 0;
    this.message =3D 'click on the board to begin';

    this.starttime =3D null;
    this.wintime =3D null;
    this.board =3D new Array();

    // create the squares
    for (var x =3D 0; x &lt; this.height; x++) {
        var row =3D new Array();
        for (var y =3D 0; y &lt; this.width; y++) {
            row.push( {count: 0, mine: false, clicked: false, marked: false=
} );
        }
        this.board.push(row);
    }

    // add mines
    for (var m =3D 0; m &lt; this.mines; m++) {
        var mx =3D Math.round((this.height-1)*Math.random());
        var my =3D Math.round((this.width-1)*Math.random());
        if (this.board[mx][my].mine) {
            m--;
        } else {
            this.board[mx][my].mine =3D true;
        }
    }

    // work out counts
    for (var cx =3D 0; cx &lt; this.height; cx++) {
        for (var cy =3D 0; cy &lt; this.width; cy++) {
            var count =3D 0;
            for (var dx =3D -1; dx &lt; 2; dx++) {
                for (var dy =3D -1; dy &lt; 2; dy++) {
                    var nx =3D cx + dx;
                    var ny =3D cy + dy;
                    if ( (!(dx=3D=3D0 &amp;&amp; dy=3D=3D0))
                            &amp;&amp; (nx &gt;=3D 0) &amp;&amp; (nx &lt; t=
his.height)
                            &amp;&amp; (ny &gt;=3D 0) &amp;&amp; (ny &lt; t=
his.width)
                            &amp;&amp; this.board[nx][ny].mine) {
                        count++;
                    }
                }
            }
            this.board[cx][cy].count =3D count;
        }
    }
    this.showBoard();
}
// -----------------------------------------------------------------------
MinesweeperGame.prototype.showBoard =3D function() {
    var node =3D document.getElementById(this.id);
    var html =3D new Array();
    html.push('&lt;table class=3D"minefield" cellspacing=3D"2"&gt;');
    html.push('&lt;tr&gt;&lt;td class=3D"info" colspan=3D"'+this.width+'"&g=
t;');
    html.push('Minesweeper '+this.version+'&lt;br /&gt;&lt;b&gt;'+this.game=
state+'&lt;/b&gt;');
    if (this.gamestate =3D=3D 'playing') {
        this.message =3D (this.mines - this.marks)+' mines unmarked';
    }
    html.push('&lt;/td&gt;&lt;/tr&gt;');
    for (var x =3D 0; x &lt; this.height; x++) {
        html.push('&lt;tr&gt;');
        for (var y =3D 0; y &lt; this.width; y++) {
            html.push( this.makeSquare(x,y) );
        }
        html.push('&lt;/tr&gt;');
    }
    var cls =3D 'un';
    if (this.markMode) cls =3D '';
    html.push('&lt;tr&gt;&lt;td id=3D"'+this.id+'_markbtn" class=3D"'+cls+'=
clicked widebtn" colspan=3D"'+this.width+'"&gt;mark / unmark mines&lt;/td&g=
t;&lt;/tr&gt;');
    html.push('&lt;tr&gt;&lt;td class=3D"info" colspan=3D"'+this.width+'"&g=
t;'+this.message);
    html.push('&lt;small&gt;');
    html.push('&lt;br /&gt;&lt;span class=3D"minesweeper' + cls + 'show"&gt=
;ctrl- shift- or alt-&lt;/span&gt;click to reveal a square');
    html.push('&lt;br /&gt;&lt;span class=3D"minesweeper' + cls + 'hide"&gt=
;ctrl- shift- or alt-&lt;/span&gt;click to mark a mine');
    html.push('&lt;/small&gt;');
    html.push('&lt;/td&gt;&lt;/tr&gt;');
    html.push('&lt;/table&gt;');
    node.innerHTML =3D html.join('');
    node.onclick =3D this.getClickHandler();
}
// -----------------------------------------------------------------------
MinesweeperGame.prototype.makeSquare =3D function(x,y) {
    var sq =3D this.board[x][y];
    var reveal =3D (this.gamestate !=3D 'playing' &amp;&amp; this.gamestate=
 !=3D 'ready');
    var html =3D new Array();
    if (sq.clicked) {
        html.push('&lt;td class=3D"clicked" id=3D"'+this.id+'_x-'+x+'_y-'+y=
+'"&gt;');
        if (!sq.marked &amp;&amp; reveal &amp;&amp; sq.mine) {
            html.push('B!');
        } else if (!sq.marked &amp;&amp; reveal &amp;&amp; sq.mine) {
            html.push('B!');
        } else if (sq.count &gt; 0){
            html.push(sq.count);
        } else {
            html.push('&amp;nbsp;');
        }
    } else {
        html.push('&lt;td class=3D"unclicked" id=3D"'+this.id+'_x-'+x+'_y-'=
+y+'"&gt;');
        if (sq.marked &amp;&amp; !reveal) {
            html.push('B?');
        } else if (sq.marked &amp;&amp; sq.mine &amp;&amp; reveal) {
            html.push('B');
        } else if (sq.marked &amp;&amp; !sq.mine &amp;&amp; reveal) {
            html.push('X');
        } else if (sq.mine &amp;&amp; reveal) {
            html.push('B!');
        } else {
            html.push('&amp;nbsp;');
        }
    }
    html.push('&lt;/td&gt;');
    return html.join('');
}
// -------------------------------------------------------------------
MinesweeperGame.prototype.clickSquare =3D function(cx,cy,modifier) {
    if (this.gamestate =3D=3D 'ready') {
        this.starttime =3D new Date();
        this.gamestate =3D 'playing';
    }
    if (this.gamestate =3D=3D 'playing') {
        if (!this.board[cx][cy].clicked) {
            if ( (modifier &amp;&amp; !this.markMode) || (!modifier &amp;&a=
mp; this.markMode) ) {
                if (this.board[cx][cy].marked) {
                    this.marks--;
                    this.board[cx][cy].marked =3D false;
                } else {
                    this.marks++;
                    this.board[cx][cy].marked =3D true;
                }
            } else if (!this.board[cx][cy].clicked &amp;&amp; !this.board[c=
x][cy].marked) {
                this.revealSquare(cx,cy);
            }
            this.markMode =3D false;
        }
        this.checkWin();
        this.showBoard();
    } else {
        // clicked when we're not playing..
        this.startGame();
    }
}
// -------------------------------------------------------------------
MinesweeperGame.prototype.revealSquare =3D function(x,y) {
    if (this.board[x][y].clicked =3D=3D false &amp;&amp; this.board[x][y].m=
arked =3D=3D false) {
        this.board[x][y].clicked =3D true;
        this.clicks++;
        if (this.board[x][y].mine) {
            this.gamestate =3D 'boom!';
            this.message =3D 'click board to play again';
        } else if (this.board[x][y].count =3D=3D 0) {
            // if it's a zero, we might have to reveal some other squares..
            for (var dx =3D -1; dx &lt; 2; dx++) {
                for (var dy =3D -1; dy &lt; 2; dy++) {
                    var nx =3D x + dx;
                    var ny =3D y + dy;
                    if ( (!(dx=3D=3D0 &amp;&amp; dy=3D=3D0)) &amp;&amp; (nx=
 &gt;=3D 0) &amp;&amp; (nx &lt; this.height) &amp;&amp; (ny &gt;=3D 0) &amp=
;&amp; (ny &lt; this.width) ) {
                        this.revealSquare(nx,ny);
                    }
                }
            }
        }
    }
}
// -------------------------------------------------------------------
MinesweeperGame.prototype.handleClick =3D function(e) {
    // work out which cell was clicked
    if (!e) var e =3D window.event;
    var str =3D resolveTarget(e).id;
    if (str &amp;&amp; str !=3D undefined) {
        if (str =3D=3D this.id + '_markbtn') {
            this.markMode =3D !this.markMode;
            this.showBoard();
        } else {
            var cx =3D parseInt(str.substr( str.indexOf('x-')+2 ));
            var cy =3D parseInt(str.substr( str.indexOf('y-')+2 ));
            if ( !isNaN(cx) &amp;&amp; !isNaN(cy) ) {
                this.clickSquare(cx,cy,(e.altKey || e.shiftKey || e.ctrlKey=
));
            }
        }
    }
}
// -------------------------------------------------------------------
MinesweeperGame.prototype.getClickHandler =3D function() {
    var thisGame =3D this;
    return function(e) {
        thisGame.handleClick(e);
    }
}
// -------------------------------------------------------------------
MinesweeperGame.prototype.checkWin =3D function() {
    if (this.clicks =3D=3D this.total - this.mines &amp;&amp; this.gamestat=
e !=3D'boom!') {
        this.gamestate =3D 'win';
        this.wintime =3D new Date();
        this.message =3D 'You won in '+Math.round(((this.wintime - this.sta=
rttime)/100))/10+' seconds';
        this.message =3D this.message + '&lt;br /&gt;click board to play ag=
ain';
    }
}

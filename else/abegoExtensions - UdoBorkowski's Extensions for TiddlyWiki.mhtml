// Formatter

// Process the &lt;part ...&gt; ... &lt;/part&gt; starting at (w.source, w.=
matchStart) for formatting.
//
// @return true if a complete part section (including the end tag) could be=
 processed, false otherwise.
//
var handlePartSection =3D function(w) {
	var tagMatch =3D findNextPartEndOrStartTagMatch(w.source, w.matchStart);
	if (!tagMatch) return false;
	if (tagMatch.index !=3D w.matchStart || !tagMatch[2]) return false;

	// Parse the start tag parameters
	var arguments =3D parseStartTagParams(tagMatch[3]);
	if (!arguments) return false;
=09
	// Continue processing
	var startTagEndIndex =3D skipEmptyEndOfLine(w.source, tagMatch.index + tag=
Match[0].length);
	var endMatch =3D findNextPartEndOrStartTagMatch(w.source, startTagEndIndex=
);
	if (endMatch &amp;&amp; endMatch[1]) {
		if (!arguments.isHidden) {
			w.nextMatch =3D startTagEndIndex;
			w.subWikify(w.output,partEndTagREString);
		}
		w.nextMatch =3D skipEmptyEndOfLine(w.source, endMatch.index + endMatch[0]=
.length);
	=09
		return true;
	}
	return false;
}

